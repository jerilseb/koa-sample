{
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
      "no-use-before-define": "off",
      "no-multi-str": 0,
      "arrow-parens": 0,
      "no-restricted-syntax": "off",
      "quotes": "off",
      "comma-dangle": "off",
      "linebreak-style": "off"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "extends": [
      "airbnb-base"
    ]
  }
  